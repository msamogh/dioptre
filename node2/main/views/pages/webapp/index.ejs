                <!-- /.row -->

                <div class="row">
                    <div id='morning' class="col-lg-6">
                        <h2>Morning</h2>
                        <div>
                            <ul>
                                <% todos.forEach(function(todo) { %>
                                    <li class="ui-state-default">
                                        <div class="checkbox">
                                            <label>
                                                <input type="checkbox" value="" /><%=todo.taskName%></label>
                                        </div>
                                    </li>
                                <% }); %>
                            </ul>
                        </div>
                    </div>
                    <div id='afternoon' class="col-lg-6">
                        <h2>Afternoon</h2>
                        <div>
                            <ul>
                                <% for (var i = 0; i < user_stats.count; i++) { %>
                                    <li class="ui-state-default bg-info">
                                        <div class="checkbox">
                                            <label>
                                                <input type="checkbox" value="" />Buy bread</label>
                                        </div>
                                    </li>
                                <% } %>
                                 <li class="ui-state-default bg-warning">
                                        <div class="checkbox">
                                            <label>
